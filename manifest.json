{
  "name": "Relevant Bookmarks",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Gives you relevant bookmarks specific to the page you are in.",
  "homepage_url": "https://kunalpanchal.in",
  "icons": {
    "16": "icons/rb-16.png",
    "128": "icons/rb-128.png"
  },
  "browser_action": {
    "default_icon": "icons/rb-128.png",
    "default_title": "Relevant Bookmarks",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ]
}